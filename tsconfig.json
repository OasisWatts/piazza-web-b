{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "module": "commonjs",
    "target": "es2017",
    "jsx": "react",
    "baseUrl": "src",
    "sourceMap": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "paths": {
      "reactNative": [
        "../node_modules/react-native/types/index"
      ]
    }
  },
  "include": [
    "src",
    "src/front/ssr/navigator.tsx"
  ]
}